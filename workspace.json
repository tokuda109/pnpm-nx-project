{
  "version": 2,
  "projects": {
    "app": {
      "root": "apps/app",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:run-script",
          "outputs": ["apps/app/.nuxt"],
          "options": {
            "script": "build"
          }
        },
        "generate": {
          "executor": "@nrwl/workspace:run-script",
          "outputs": ["dist"],
          "dependsOn": [
            {
              "target": "build",
              "projects": "self"
            }
          ],
          "options": {
            "script": "generate"
          }
        },
        "start": {
          "executor": "@nrwl/workspace:run-script",
          "dependsOn": [
            {
              "target": "generate",
              "projects": "self"
            }
          ],
          "options": {
            "script": "start"
          }
        }
      }
    },
    "e2e": {
      "root": "apps/e2e",
      "targets": {
        "run": {
          "executor": "@nrwl/workspace:run-script",
          "dependsOn": [
            {
              "target": "generate",
              "projects": "dependencies"
            }
          ],
          "options": {
            "script": "run"
          }
        }
      },
      "implicitDependencies": ["app"]
    }
  }
}
